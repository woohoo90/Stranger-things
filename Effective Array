# Welcome to Sonic Pi
#Effective Array
x = 0
alphabet = ["a","b","c","d","e","f","g","h","i","j","k","l","m","n","o","p","q","r","s","t","u","v","w","x","y","z","!","Now I know that you know that I know my ABCs"]

live_loop :letters do
  28.times do
    print(alphabet[x])
    sleep(0.25)
    x = x + 1
  end
  x = 0
end
